ratings=../dataset/douban/ratings.txt
social=../dataset/FilmTrust/trust.txt
ratings.setup=-columns 0 1 2 -header
social.setup=-columns 0 1 -header
recommender=RSGAN
evaluation.setup=-cv 5 -b 1
item.ranking=on -topN 10
num.factors=50
num.max.iter=5
batch_size=100
learnRate=-init 0.001 -max 1
reg.lambda=-u 0.001 -i 0.01 -b 0.2 -s 0.2
output.setup=on -dir ../results/
